{
  "family": "words",
  "version": "1.0.0",
  "plugins": [
    {
      "id": "aspose-words-cloud",
      "name": "Aspose.Words Cloud",
      "slug": "aspose-words-cloud",
      "description": "Cloud API for Word document processing",
      "patterns": {
        "classNames": ["WordsApi", "WordsDocument"],
        "methods": ["GetDocument", "SaveDocument", "ConvertDocument"],
        "imports": ["aspose.words.cloud"]
      },
      "family": "words",
      "version": "21.10.0",
      "dependencies": [],
      "capabilities": ["convert", "edit", "render"],
      "plugin_type": "api",
      "load_formats": ["DOC", "DOCX", "RTF", "ODT"],
      "save_formats": ["PDF", "HTML", "DOCX", "TXT"]
    },
    {
      "id": "aspose-words-net",
      "name": "Aspose.Words for .NET",
      "slug": "aspose-words-net",
      "description": ".NET library for Word document manipulation",
      "patterns": {
        "classNames": ["Document", "DocumentBuilder", "NodeCollection"],
        "methods": ["Save", "LoadDocument", "InsertHtml"],
        "imports": ["Aspose.Words"]
      },
      "family": "words",
      "version": "21.10.0",
      "dependencies": [],
      "capabilities": ["convert", "edit", "mail_merge"],
      "plugin_type": "library",
      "load_formats": ["DOC", "DOCX", "RTF", "ODT", "HTML"],
      "save_formats": ["PDF", "DOCX", "HTML", "EPUB", "XPS"]
    }
  ],
  "combination_rules": [
    {
      "name": "cloud_and_local",
      "plugins": ["aspose-words-cloud", "aspose-words-net"],
      "trigger_patterns": ["hybrid", "sync"],
      "confidence_boost": 0.2,
      "required_all": false
    }
  ],
  "required_fields": ["title", "description"],
  "forbidden_patterns": ["deprecated_api", "insecure_method"],
  "metadata": {
    "last_updated": "2024-01-15",
    "maintainer": "Aspose Documentation Team"
  }
}
